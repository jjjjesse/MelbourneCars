<div class="jumbotron">
<div class="jumbotron">

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Make model</th>
      <th>Year</th>
      <th>Kms</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= image_tag car.image.url(:medium) %></td>
        <td><%= car.make_model %></td>
        <td><%= car.year %></td>
        <td><%= car.kms %></td>
        <td><%= car.description %></td>
        <td><%= car.price %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>

</div>

</div>